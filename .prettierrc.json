{
    "printWidth": 100,
    "tabWidth": 2,
    "bracketSpacing": true,
    "trailingComma": "es5",
    "bracketSameLine": false,
    "arrowParens": "always",
    "semi": true,
    "endOfLine": "auto",
    "importOrder": [
        "^@cucumber(.*)$",
        "^playwright(.*)$",
        "^axios(.*)$",
        "^graphql(.*)$",
        "^@flutter/(.*)$",
        "^@drivers/(.*)$",
        "^@supports/(.*)$",
        "^@services/(.*)$",
        "^@graphql/(.*)$",
        "^[./a-z\\-/]"
    ],
    "importOrderSeparation": true,
    "singleQuote": true
}